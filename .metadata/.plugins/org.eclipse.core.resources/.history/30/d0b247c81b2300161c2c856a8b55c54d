from tkinter import *

class TourGui():
    
    def __init__(self):
        
        # Initiates GUI
        root = Tk()
        root.wm_title('Tour')
        root.configure(background = 'white')
        
        # Top Frame
        topFrame = Frame(root)
        
        # Origin
        originFr = Frame(topFrame)
        orLabel = Label(originFr)
        
        # Bottom Frame
        botFrame = Frame(root)